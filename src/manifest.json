{
  "name": "PKU Gateway",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "北京大学网关插件 2.0",
  "icons": {
    "16": "public/img/icon16.png",
    "48": "public/img/icon48.png",
    "128": "public/img/icon128.png"
